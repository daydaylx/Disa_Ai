{
  "name": "disa-ai",
  "version": "1.0.2",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:functions": "wrangler pages dev dist --live-reload",
    "generate-tokens": "node scripts/generate-tokens.mjs",
    "prebuild": "node scripts/build-info.js && npm run generate-tokens",
    "build": "vite build",
    "build:debug": "DEBUG_SOURCEMAP=1 vite build --mode development",
    "preview": "node ./scripts/run-preview.mjs",
    "preview:functions": "wrangler pages dev dist",
    "typecheck": "npm run typecheck:build && npm run typecheck:test",
    "typecheck:build": "tsc -p tsconfig.build.json --noEmit",
    "typecheck:test": "tsc -p tsconfig.test.json --noEmit",
    "lint": "eslint .",
    "lint:css": "stylelint \"**/*.css\" && node ./tools/check-css-hex.mjs",
    "lint:fix": "eslint . --fix",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:smoke": "vitest run tests/smoke --coverage.enabled=false",
    "test:config": "vitest run tests/build",
    "test:build": "node scripts/test-build.mjs",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:live": "PLAYWRIGHT_LIVE=1 playwright test tests/e2e/live",
    "e2e:smoke": "playwright test tests/e2e/chat.smoke.spec.ts",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "analyze": "npx vite-bundle-analyzer",
    "release:check": "npm run changeset:status",
    "release:preview": "npm run changeset:status && npm run build",
    "release:prepare": "npm run verify && npm run changeset:version",
    "verify": "npm run typecheck && npm run lint && npm run test:unit",
    "clean": "rm -rf dist .vite .cache .tmp coverage test-results playwright-report rendered.html",
    "postbuild": "cp public/_headers dist/_headers && cp -r functions dist/functions && node scripts/generate-routes.js && node scripts/verify-dist.mjs",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production') process.exit(1)\" || husky install",
    "lh": "lhci autorun --config=./lighthouserc.cjs",
    "lint-staged": "lint-staged",
    "verify:dist": "node scripts/verify-dist.mjs",
    "mcp:visual": "npx tsx tools/zai-vision-mcp.ts",
    "mcp:setup": "bash scripts/setup-mcp.sh",
    "mcp:test": "npx @modelcontextprotocol/server-filesystem --help && npx @modelcontextprotocol/server-git --help",
    "mcp:update": "npm update @modelcontextprotocol/server-* --save-dev",
    "jules:docs": "cat .claude/JULES_WORKFLOWS.md",
    "jules:quick": "cat .claude/JULES_QUICK_REFERENCE.md",
    "jules:help": "echo 'ðŸ“š Jules Documentation:' && echo '  npm run jules:docs     - VollstÃ¤ndige Workflows' && echo '  npm run jules:quick    - Quick Reference' && echo '  cat .claude/JULES_INTEGRATION.md - Setup Guide'"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ]
  },
