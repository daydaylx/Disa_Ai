{
  "name": "disa-ai",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/build-info.js",
    "build": "vite build",
    "build:debug": "DEBUG_SOURCEMAP=1 vite build --mode development",
    "preview": "node ./scripts/run-preview.mjs",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:css": "node ./tools/check-css-hex.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run",
    "e2e": "playwright test",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "analyze": "npx vite-bundle-analyzer",
    "release:check": "npm run changeset:status",
    "release:preview": "npm run changeset:status && npm run build",
    "release:prepare": "npm run verify && npm run changeset:version",
    "verify": "npm run typecheck && npm run lint && npm run test:unit && npm run e2e",
    "clean": "rm -rf dist .vite .cache .tmp coverage test-results",
    "postbuild": "cp public/_redirects dist/_redirects && cp public/_headers dist/_headers && node scripts/generate-routes.js",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production') process.exit(1)\" || husky install",
    "lh": "lhci autorun --config=./lighthouserc.cjs",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
