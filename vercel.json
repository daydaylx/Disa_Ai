{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/persona.json",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    },
    {
      "source": "/personas.json",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
